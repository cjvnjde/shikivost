{
  "name": "bridge",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "extension/bridge/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nx/rollup:rollup",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/extension/bridge",
        "main": "extension/bridge/src/index.ts",
        "tsConfig": "extension/bridge/tsconfig.lib.json",
        "assets": [],
        "project": "extension/bridge/package.json",
        "compiler": "swc",
        "format": ["cjs", "esm"]
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"]
    }
  },
  "tags": []
}
