{
  "name": "@shikivost/source",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "build:extension:static": "nx run static:build --skip-nx-cache",
    "build:extension:content": "nx run content:build --skip-nx-cache",
    "watch:extension:content": "nx run content:watch --skip-nx-cache",
    "start:extesion:firefox": "web-ext run -t firefox-desktop --source-dir ./dist/extension --start-url=https://animevost.org/",
    "serve:extension:firefox": "pnpm run build:extension:content && concurrently --kill-others 'pnpm run watch:extension:content' 'pnpm run start:extesion:firefox'"
  },
  "private": true,
  "devDependencies": {
    "@nx/workspace": "17.2.8",
    "@rollup/plugin-typescript": "^11.1.6",
    "concurrently": "^8.2.2",
    "nx": "17.2.8",
    "prettier": "^2.6.2",
    "rollup": "^4.9.5",
    "typescript": "~5.2.2",
    "web-ext": "^7.10.0",
    "webextension-polyfill": "^0.10.0"
  },
  "dependencies": {
    "tslib": "^2.6.2"
  }
}
