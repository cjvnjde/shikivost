{
  "name": "@shikivost/source",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "build:extension:static": "nx run static:build --skip-nx-cache",
    "build:extension:content": "nx run content:build --skip-nx-cache",
    "watch:extension:content": "nx run content:watch --skip-nx-cache",
    "build:extension:popup": "nx run popup:build --skip-nx-cache",
    "watch:extension:popup": "nx run popup:watch --skip-nx-cache",
    "build:extension:background": "nx run background:build --skip-nx-cache",
    "watch:extension:background": "nx run background:watch --skip-nx-cache",
    "start:extension:firefox": "web-ext run -t firefox-desktop --source-dir ./dist/extension --start-url=https://animevost.org/",
    "serve:extension:firefox": "pnpm run build:extension:content && pnpm run build:extension:popup  && pnpm run build:extension:background && concurrently --kill-others 'pnpm run watch:extension:popup' 'pnpm run watch:extension:content' 'pnpm run watch:extension:background' 'pnpm run start:extension:firefox'",
    "build:libs:bridge": "nx run bridge:build --skip-nx-cache"
  },
  "private": true,
  "devDependencies": {
    "@nx/workspace": "17.2.8",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-html": "^1.0.3",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-replace": "^5.0.5",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^11.1.6",
    "@types/react": "^18.2.47",
    "@types/react-dom": "^18.2.18",
    "@types/webextension-polyfill": "^0.10.7",
    "concurrently": "^8.2.2",
    "nx": "17.2.8",
    "prettier": "^2.6.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rollup": "^4.9.5",
    "typescript": "~5.2.2",
    "web-ext": "^7.10.0",
    "webextension-polyfill": "^0.10.0"
  },
  "dependencies": {
    "tslib": "^2.6.2"
  }
}
